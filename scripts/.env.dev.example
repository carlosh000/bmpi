# Copia este archivo como scripts/.env.dev y completa valores locales.
# Solo para desarrollo local.

BMPI_ENV=development

# DB para backend + IA
DB_HOST=localhost
DB_NAME=bmpi
DB_USER=postgres
DB_PASSWORD=REEMPLAZAR_PASSWORD_DB_LOCAL
DB_SSLMODE=disable

# Opcional en dev
BMPI_HTTP_ADDR=:8080
BMPI_FACE_GRPC_ADDR=127.0.0.1:50051
BMPI_FACE_GRPC_TLS=false
BMPI_ALLOWED_ORIGINS=http://localhost:4200,http://127.0.0.1:4200
